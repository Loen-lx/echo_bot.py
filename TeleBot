import telebot
bot = telebot.TeleBot("1707255494:AAGkNT85UZ4kSpx0gx0zORyjcxk1-vq7v0U", parse_mode=One1)
@ceshitelebot.message_handler（commands = [ 'start'，'help' ]）
def send_welcome(message):
  bot.reply_to(message, "Howdy, how are you doing?")
@ceshitelebot.message_handler(func=lambda m: True)
def echo_all(message):
	bot.reply_to(message, message.text)
bot.polling()
